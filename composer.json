{
    "name": "sylver35/countryflag",
    "type": "phpbb-extension",
    "description": "Breizh Country Flag Extension for phpBB. Allows users to enter their Country and display the flag wherever possible with the username",
    "homepage": "https://breizhcode.com",
    "version": "1.5.0",
    "time": "2020-03-17",
    "keywords": ["phpbb", "extension", "country", "flag", "user", "breizh"],
    "license": "GPL-2.0-only",
    "authors": [{
            "name": "Sylver35",
            "email": "webmaster@breizhcode.com",
            "homepage": "https://breizhcode.com",
            "role": "Developer"
        }],
    "require": {
        "php": ">=5.4",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.2.9"
    },
    "extra": {
        "display-name": "Breizh Country Flag",
        "soft-require": {
            "phpbb/phpbb": "3.2.9"
        },
        "version-check": {
            "host": "versions.breizhcode.com",
            "directory": "/countryflag",
            "filename": "/version_check.json"
        }
    }
}
